<!DOCTYPE html>
<html>
<head>
	<title><%= title %> | Artist Index </title>
  <%- include('stylesheets') %>
 </head>
 <body>
  <h1>Artist Index</h1>
  <a href="/artist/new">New Search</a>
   <table>
    <thead>
     <tr>
  	 <th>Artist</th>
  	 <th>Genre</th>
    </tr>
   </thead>
  <tbody>
  <% artists.forEach(function (artist) { %>
   <tr>
   	<td><%= artist.artist %></td>
   	<td><%= artist.genre %></td>
   	<td>
   	<form action="/artist/delete/<%= artist._id %>" name="delete" method="POST">
   	<input type="submit" value="delete">
   	</form>
   	<form action="/artist/<%= artist._id %>/submit" method="POST">
   	</form>
	</td>
	</tr>
   <% }); %>
   </tbody>
  </table>
  <%- include('javascript') %>
 </body>
</html>